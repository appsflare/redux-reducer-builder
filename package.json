{
  "name": "@appsflare/redux-reducer-builder",
  "version": "1.0.8",
  "description": "An easy to use utility to build type-safe reducers",
  "main": "dist/index.js",
  "module": "es/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clear": "rimraf ./dist ./es",
    "release": "npm publish --access public",
    "prepublish": "npm run clear && npm run build",
    "build": "tsc && tsc --module esnext --outDir ./es",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appsflare/redux-reducer-builder.git"
  },
  "keywords": [
    "redux",
    "reducer",
    "builder",
    "type ssfe",
    "type safety",
    "typescript"
  ],
  "author": "me@vjsrinath.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appsflare/redux-reducer-builder/issues"
  },
  "homepage": "https://github.com/appsflare/redux-reducer-builder#readme",
  "dependencies": {
    "redux": "^3.7.2",
    "typescript": "^2.7.2"
  },
  "devDependencies": {
    "rimraf": "^2.6.2"
  }
}
